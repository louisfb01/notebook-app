{
    "appName": "notebook-app",
    "instanceCount": 1,
    "notExposeAsWebApp": false,
    "websocketSupport": true,
    "forceSsl": true,
    "containerHttpPort": "8888",
    "envVars": [
        {
            "key": "CODA_AUTH_SERVICE_URL",
            "value": "$CODA_AUTH_SERVICE_URL"
        },
        {
            "key": "CODA_NOTEBOOK_APP_AUTH_REALM",
            "value": "$CODA_NOTEBOOK_APP_AUTH_REALM"
        },
        {
            "key": "CODA_HUB_API_URL",
            "value": "$CODA_HUB_API_URL"
        },
        {
            "key": "CODA_NOTEBOOK_APP_AUTH_CLIENT_ID",
            "value": "$CODA_NOTEBOOK_APP_AUTH_CLIENT_ID"
        },
        {
            "key": "CODA_NOTEBOOK_APP_AUTH_CLIENT_SECRET",
            "value": "$CODA_NOTEBOOK_APP_AUTH_CLIENT_SECRET"
        }
    ]
}